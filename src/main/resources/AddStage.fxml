<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="contactdatabasegui.AddStageController">

    <center>
        <VBox prefHeight="385.0" prefWidth="502.0" BorderPane.alignment="CENTER">
            <children>
                <Label contentDisplay="CENTER" prefHeight="17.0" prefWidth="67.0" text="First Name"
                       textAlignment="RIGHT">
                    <VBox.margin>
                        <Insets left="10.0" top="10.0"/>
                    </VBox.margin>
                </Label>
                <TextField fx:id="firstNameField">
                    <VBox.margin>
                        <Insets bottom="5.0" left="10.0" right="300.0" top="10.0"/>
                    </VBox.margin>
                </TextField>
                <Label text="Last Name">
                    <VBox.margin>
                        <Insets left="10.0"/>
                    </VBox.margin>
                </Label>
                <TextField fx:id="lastNameField">
                    <VBox.margin>
                        <Insets bottom="5.0" left="10.0" right="300.0" top="5.0"/>
                    </VBox.margin>
                </TextField>
                <Label text="Primary Email">
                    <VBox.margin>
                        <Insets left="10.0"/>
                    </VBox.margin>
                </Label>
                <TextField fx:id="primaryEmailField">
                    <VBox.margin>
                        <Insets bottom="5.0" left="10.0" right="300.0" top="5.0"/>
                    </VBox.margin>
                </TextField>
                <Label text="Secondary Email">
                    <VBox.margin>
                        <Insets left="10.0"/>
                    </VBox.margin>
                </Label>
                <TextField fx:id="secondaryEmailField">
                    <VBox.margin>
                        <Insets bottom="5.0" left="10.0" right="300.0" top="5.0"/>
                    </VBox.margin>
                </TextField>
                <Label text="Primary Phone">
                    <VBox.margin>
                        <Insets left="10.0"/>
                    </VBox.margin>
                </Label>
                <TextField fx:id="primaryPhoneField">
                    <VBox.margin>
                        <Insets bottom="5.0" left="10.0" right="300.0" top="5.0"/>
                    </VBox.margin>
                </TextField>
                <Label text="Secondary Phone">
                    <VBox.margin>
                        <Insets left="10.0"/>
                    </VBox.margin>
                </Label>
                <TextField fx:id="secondaryPhoneField">
                    <VBox.margin>
                        <Insets bottom="5.0" left="10.0" right="300.0" top="5.0"/>
                    </VBox.margin>
                </TextField>
                <HBox alignment="CENTER_LEFT" prefHeight="58.0" prefWidth="502.0">
                    <children>
                        <Button alignment="TOP_CENTER" mnemonicParsing="false" text="Add" onAction="#onActionAdd">
                            <HBox.margin>
                                <Insets left="10.0"/>
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" text="Cancel" onAction="#onActionCancel">
                            <HBox.margin>
                                <Insets left="100.0"/>
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </center>
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="back"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>

</BorderPane>
